
extends components

block variables
  - var pageID = 'javascript/images'
  - var pageTitle = 'Images'
  - var prism = true

block article
  h3.ui-header-primary Images
  section
    h4.ui-header-secondary Lazy Loading
    div
      figure
        img.ui-image-thumbnail(width='600' height='600' src='http://oss.arxitics.com/images/emoji/camping.svg' alt='camping')
      figure.ui-skip-medium
        img.ui-image-thumbnail(width='600' height='600' src='http://oss.arxitics.com/images/emoji/white-circle.svg' alt='park' data-schema-lazyload data-schema-src='http://oss.arxitics.com/images/emoji/park.svg')
      figure.ui-skip-medium
        img.ui-image-thumbnail(width='600' height='600' src='http://oss.arxitics.com/images/emoji/white-circle.svg' alt='beach' data-schema-lazyload='200' data-schema-delay='2000' data-schema-src='http://oss.arxitics.com/images/emoji/beach.svg')
      figure.ui-skip-medium
        img.ui-image-thumbnail(width='600' height='600' src='http://oss.arxitics.com/images/emoji/white-circle.svg' alt='island' data-schema-lazyload='200' data-schema-delay='2000' data-schema-src='http://oss.arxitics.com/images/emoji/island.svg')
      figure.ui-skip-medium
        img.ui-image-thumbnail(width='600' height='600' src='http://oss.arxitics.com/images/emoji/white-circle.svg' alt='desert' data-schema-lazyload='200' data-schema-delay='2000'  data-schema-src='http://oss.arxitics.com/images/emoji/desert.svg')
    pre: code.language-markup.
      &lt;img width="600" height="600" src="http://oss.arxitics.com/images/emoji/white-circle.svg" alt="desert" data-schema-lazyload="200" data-schema-delay="2000" data-schema-src="http://oss.arxitics.com/images/emoji/desert.svg"&gt;
