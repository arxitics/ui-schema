
extends components

block variables
  - var pageID = 'javascript/icons'
  - var pageTitle = 'Icons'

mixin icon(name, alias)
  .ui-grid-25
    i.ui-fixed-width(data-schema-icon=name)
    |  
    span= name
      if alias
        |  
        span.ui-text-muted (alias)

