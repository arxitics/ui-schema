
extends links

block variables
  - var pageID = 'download'
  - var pageTitle = 'Download'

block article
  h3.ui-header-primary Download
  ul
    - var archive = 'https://github.com/arxitics/ui-schema/archive'
    - var release = '0.3.6'
    li: a(href='#{archive}/master.zip') Latest development version from GitHub
    li: a(href='#{archive}/#{release}.zip') UI Schema production version #{release}
